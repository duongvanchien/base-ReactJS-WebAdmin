{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Assets/*": ["./src/Assets/*"],
      "@Components/*": ["./src/Components/*"],
      "@Constant/*": ["./src/Constant/*"],
      "@Navigation/*": ["./src/Navigation/*"],
      "@Redux/*": ["./src/Redux/*"],
      "@Styles/*": ["./src/Styles/*"],
      "@Utils/*": ["./src/Utils/*"]
